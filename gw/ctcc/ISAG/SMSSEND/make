gcc34 -o SendSms SendSms.c soapC.c soapClient.c ../../../../gsoap-2.7/gsoap/stdsoap2.c ../../pub/pubfunc.c  ../../pub/mysqlfunc.c -I /usr/local/mysql/include/ -L /usr/local/mysql/lib/ -lmysqlclient -lgsoap -lssl -lccl -DDEBUG 


gcc34 daemon.c ../../pub/pubfunc.c ../../pub/mysqlfunc.c -o SendSmsdaemon  -I /usr/local/mysql/include/ -L /usr/local/mysql/lib/ -lmysqlclient -lccl


cp SendSms SendSmsdaemon ../../../bin -f


